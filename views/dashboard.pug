extends base.pug
block content
    a.btn.btn-primary(href="/addtestbycat") Saisir un test par Campagne

    hr
    - if(testslist)
        table#dataTable.table.table-striped
            thead
                tr
                    th Fermé
                    th Description du test
                    th Statut du test
                    th Catégorie du test
                    th
                    th
            each test in testslist
                tr
                    td
                        - if(test.endDate)
                            | Fermé
                    td=test.tDescription
                    td=test.statut
                    td=test.catLabel
                    td
                        a.btn.btn-primary(href="/addissue/"+test.tId) Saisir anomalie
                    td
                        a.btn.btn-danger(href="/changestatut/"+test.tId+"/"+test.cId) Changer statut
    - if(rank == 3)
        .row
            .card.shadow.mb-4.col-6
                .card-header.py-3
                    h6.m-0.font-weight-bold.text-primary Nombre de tests par mois
                .card-body.embed-responsive.embed-responsive-16by9
                    .text-center
                        iframe(width='800', height='600', src='https://app.powerbi.com/view?r=eyJrIjoiMWU5ZWVmOTQtNTYzOC00OGE2LWI0NDQtNzk5MDZiYjI5MmU0IiwidCI6ImIzMjNiY2I0LTZkNTgtNGYyNS04N2JmLTYzNjZjM2Q2ODlhZiIsImMiOjh9', frameborder='0', allowfullscreen='true')
            .card.shadow.mb-4.col-6
                .card-header.py-3
                    h6.m-0.font-weight-bold.text-primary Nombre de tests par catégorie
                .card-body.embed-responsive.embed-responsive-16by9
                    .text-center
                        iframe.embed-responsive-item(src='https://app.powerbi.com/view?r=eyJrIjoiODdiMzRjZWEtYTQzNC00Njc1LTg2MzUtNjEwMjNiODc1NTA2IiwidCI6ImIzMjNiY2I0LTZkNTgtNGYyNS04N2JmLTYzNjZjM2Q2ODlhZiIsImMiOjh9', frameborder='0', allowfullscreen='true')
